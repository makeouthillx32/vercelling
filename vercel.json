{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" },
    { "src": "api/*.mjs", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/youtube",
      "dest": "/api/youtube",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
